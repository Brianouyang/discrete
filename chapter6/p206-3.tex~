   \node[specification] (C)  at (1.5,1.5)  {};
   \node[specification] (D) at (1.5,0)  {};
   \draw[thick] (A) to (B);
   \draw[thick] (B) to (D);
   \draw[thick] (D) to (C);
      \draw[thick] (C) to (A);
 \end{tikzpicture}\\
 \vspace*{0.5cm}
 I
\end{minipage}
  \begin{minipage}{0.24\linewidth}
    \centering
    \begin{tikzpicture}[auto,
    specification/.style ={circle, draw, thick}]
   \node[specification] (A) at (0,0)  {};
   \node[specification] (B) at (0,1.5)  {};
   \node[specification] (C) at (1.5,1.5)  {};
   \node[specification] (D) at (1.5,0)  {};
   \draw[thick] (A) to  (B);
      \draw[thick] (C) to (D);
   \draw[thick] (D) to (A);
   \draw[thick] (A) to (C);
   \draw[thick] (B) to (D);
 \end{tikzpicture}\\
 \vspace*{0.5cm}
 J
\end{minipage}
  \begin{minipage}{0.24\linewidth}
    \centering
    \begin{tikzpicture}[auto,
